ild: {{ character.shield }}</div>\r\n        <div class=\"character_classDC\">Class DC: {{ character.class_dc }}</div>\r\n        <div class=\"character_perception\">Perception: {{ character.perception }}</div>\r\n        </div>\r\n    </slide>\r\n\r\n    <template #addons>\r\n      <navigation />\r\n      <pagination />\r\n    </template>\r\n  </carousel>\r\n\r\n</template>\r\n\r\n<script>\r\nimport characters from '../characters.json'\r\nimport { Carousel, Slide, Pagination, Navigation } from 'vue3-carousel';\r\nimport 'vue3-carousel/dist/carousel.css';\r\n\r\nexport default {\r\n  name: 'App',\r\n    components: {\r\n    Carousel,\r\n    Slide,\r\n    Pagination,\r\n    Navigation,\r\n  },\r\n  data() {\r\n    return {\r\n      characters: characters\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n.carousel__slide > .carousel__item {\r\n  transform: scale(1);\r\n  opacity: 0.5;\r\n  transition: 0.5s;\r\n}\r\n.carousel__slide--visible > .carousel__item {\r\n  opacity: 1;\r\n  transform: rotateY(0);\r\n}\r\n.carousel__slide--next > .carousel__item {\r\n  transform: scale(0.9) translate(-10px);\r\n}\r\n.carousel__slide--prev > .carousel__item {\r\n  transform: scale(0.9) translate(10px);\r\n}\r\n.carousel__slide--active > .carousel__item {\r\n  transform: scale(1.1);\r\n}\r\n\r\n.carousel__item {\r\n    min-height: 200px;\r\n    width: 90%;\r\n    background-color: darkred;\r\n    color: var(--vc-clr-white);\r\n    font-size: 20px;\r\n    border-radius: 8px;\r\n    text-align: left;\r\n    padding: 10px;\r\n\r\n    div {\r\n      flex: none;\r\n    }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}