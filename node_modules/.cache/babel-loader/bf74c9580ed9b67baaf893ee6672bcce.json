{"ast":null,"code":"// https://vuejsexamples.com/modern-lightweight-vue-3-carousel-component/\nimport { Carousel, Slide, Pagination, Navigation } from 'vue3-carousel';\nimport 'vue3-carousel/dist/carousel.css';\nimport sessions from '../sessions.json';\nexport default {\n  name: 'App',\n  components: {\n    Carousel,\n    Slide,\n    Pagination,\n    Navigation\n  },\n\n  data() {\n    return {\n      sessions: sessions\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAcA;AAEA,SAASA,QAAT,EAAmBC,KAAnB,EAA0BC,UAA1B,EAAsCC,UAAtC,QAAwD,eAAxD;AACA,OAAO,iCAAP;AAEA,OAAOC,QAAP,MAAqB,kBAArB;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEbC,UAAU,EAAE;IACVN,QADU;IAEVC,KAFU;IAGVC,UAHU;IAIVC;EAJU,CAFC;;EAQbI,IAAI,GAAG;IACL,OAAO;MACLH,QAAQ,EAAEA;IADL,CAAP;EAGF;;AAZa,CAAf","names":["Carousel","Slide","Pagination","Navigation","sessions","name","components","data"],"sourceRoot":"","sources":["D:\\Projekte\\eloria\\src\\components\\SliderCarousel.vue"],"sourcesContent":["<template>\n  <carousel :items-to-show=\"3.95\" :wrapAround=\"true\">\n    <slide v-for=\"session in sessions\" :key=\"session.episode\">\n      <div class=\"carousel__item\">{{ session.title }}</div>\n    </slide>\n\n    <template #addons>\n      <navigation />\n      <pagination />\n    </template>\n  </carousel>\n</template>\n\n<script>\n// https://vuejsexamples.com/modern-lightweight-vue-3-carousel-component/\n\nimport { Carousel, Slide, Pagination, Navigation } from 'vue3-carousel';\nimport 'vue3-carousel/dist/carousel.css';\n\nimport sessions from '../sessions.json'\n\nexport default {\n  name: 'App',\n  components: {\n    Carousel,\n    Slide,\n    Pagination,\n    Navigation,\n  },\n  data() {\n    return {\n      sessions: sessions\n    }\n  }\n};\n</script>"]},"metadata":{},"sourceType":"module"}