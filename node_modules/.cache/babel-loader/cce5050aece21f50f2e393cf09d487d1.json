{"ast":null,"code":"import characters from '../characters.json';\nimport { Carousel, Slide, Pagination, Navigation } from 'vue3-carousel';\nimport 'vue3-carousel/dist/carousel.css';\nexport default {\n  name: 'App',\n  components: {\n    Carousel,\n    Slide,\n    Pagination,\n    Navigation\n  },\n\n  data() {\n    return {\n      characters: characters\n    };\n  }\n\n};","map":{"version":3,"mappings":"AAuGA,OAAOA,UAAP,MAAuB,oBAAvB;AACA,SAASC,QAAT,EAAmBC,KAAnB,EAA0BC,UAA1B,EAAsCC,UAAtC,QAAwD,eAAxD;AACA,OAAO,iCAAP;AAEA,eAAe;EACbC,IAAI,EAAE,KADO;EAEXC,UAAU,EAAE;IACZL,QADY;IAEZC,KAFY;IAGZC,UAHY;IAIZC;EAJY,CAFD;;EAQbG,IAAI,GAAG;IACL,OAAO;MACLP,UAAU,EAAEA;IADP,CAAP;EAGF;;AAZa,CAAf","names":["characters","Carousel","Slide","Pagination","Navigation","name","components","data"],"sourceRoot":"","sources":["D:\\Projekte\\eloria\\src\\components\\CharacterSheets.vue"],"sourcesContent":["<template>\r\n  <!-- <div v-for=\"character in characters\" :key=\"character.name\">\r\n    <h2>{{ character.name }}</h2>\r\n  </div> -->\r\n  \r\n  <carousel :items-to-show=\"3.4\" :wrapAround=\"true\">\r\n    <slide v-for=\"character in characters\" :key=\"character.name\">\r\n      <div class=\"carousel__item\">\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Charakter:</div>\r\n          <div class=\"character_value\">{{ character.name }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Spieler:</div>\r\n          <div class=\"character_value\">{{ character.player }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Volk:</div>\r\n          <div class=\"character_value\">{{ character.ancestry }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Geschlecht:</div>\r\n          <div class=\"character_value\">{{ character.gender }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Alter:</div>\r\n          <div class=\"character_value\">{{ character.age }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Klasse:</div>\r\n          <div class=\"character_value\">{{ character.class }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Sprachen:</div>\r\n          <div class=\"character_value\">{{ character.languages }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Level:</div>\r\n          <div class=\"character_value\">{{ character.level }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Lebenspunkte:</div>\r\n          <div class=\"character_value\">{{ character.hit_points }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Rüstungsklasse:</div>\r\n          <div class=\"character_value\">{{ character.armor_class }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Schild:</div>\r\n          <div class=\"character_value\">{{ character.shield }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Class DC:</div>\r\n          <div class=\"character_value\">{{ character.class_dc }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Perception:</div>\r\n          <div class=\"character_value\">{{ character.perception }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_headline\">Rettungswürfe</div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Fortitude:</div>\r\n          <div class=\"character_value\">{{ character.fortitude }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Reflex:</div>\r\n          <div class=\"character_value\">{{ character.reflex }}</div>\r\n        </div>\r\n\r\n        <div class=\"character_item\">\r\n          <div class=\"character_key\">Wille:</div>\r\n          <div class=\"character_value\">{{ character.will }}</div>\r\n        </div>\r\n\r\n      </div>\r\n    </slide>\r\n\r\n    <template #addons>\r\n      <navigation />\r\n      <pagination />\r\n    </template>\r\n  </carousel>\r\n\r\n</template>\r\n\r\n<script>\r\nimport characters from '../characters.json'\r\nimport { Carousel, Slide, Pagination, Navigation } from 'vue3-carousel';\r\nimport 'vue3-carousel/dist/carousel.css';\r\n\r\nexport default {\r\n  name: 'App',\r\n    components: {\r\n    Carousel,\r\n    Slide,\r\n    Pagination,\r\n    Navigation,\r\n  },\r\n  data() {\r\n    return {\r\n      characters: characters\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\">\r\n.carousel__slide > .carousel__item {\r\n  transform: scale(1);\r\n  opacity: 0.5;\r\n  transition: 0.5s;\r\n}\r\n.carousel__slide--visible > .carousel__item {\r\n  opacity: 1;\r\n  transform: rotateY(0);\r\n}\r\n.carousel__slide--next > .carousel__item {\r\n  transform: scale(0.9) translate(-10px);\r\n}\r\n.carousel__slide--prev > .carousel__item {\r\n  transform: scale(0.9) translate(10px);\r\n}\r\n.carousel__slide--active > .carousel__item {\r\n  transform: scale(1.1);\r\n}\r\n\r\n.carousel__item {\r\n  min-height: 200px;\r\n  width: 90%;\r\n  background-color: darkred;\r\n  color: var(--vc-clr-white);\r\n  font-size: 20px;\r\n  border-radius: 8px;\r\n  text-align: left;\r\n  padding: 10px;\r\n\r\n  div {\r\n    flex: none;\r\n  }\r\n}\r\n\r\n.character {\r\n  &_key {\r\n    width: 30%;\r\n  }\r\n\r\n  &_key,\r\n  &_value {\r\n    display: inline-block;\r\n  }\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}